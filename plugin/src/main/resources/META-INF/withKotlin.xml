<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider language="kotlin"
                                        implementationClass="spp.jetbrains.marker.plugin.SourceLineMarkerProvider"/>
        <codeInsight.inlayProvider language="kotlin"
                                   implementationClass="spp.jetbrains.marker.plugin.SourceInlayHintProvider"/>
    </extensions>
    <extensions defaultExtensionNs="org.jetbrains.kotlin">
        <scriptDefinitionsProvider
                implementation="liveplugin.implementation.pluginrunner.kotlin.LivePluginKotlinScriptProvider"/>
    </extensions>
</idea-plugin>